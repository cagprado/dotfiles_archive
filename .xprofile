# vim: ft=sh:
# X configuration file

# Start keychain
. $HOME/.keychain.sh

# Setup screens
$HOME/bin/zsh/setmonitors
xset s off -dpms

# Set wallpaper
feh --bg-scale $HOME/usr/img/arch/wallpaper.png

# Export variables
export GTK_IM_MODULE=fcitx
export QT_IM_MODULE=fcitx
export XMODIFIERS=@im=fcitx
