! vim: ft=xdefaults
rofi.modi:                           combi,window
rofi.combi-modi:                     drun,run,ssh
rofi.width:                          60
rofi.lines:                          15
rofi.columns:                        1
rofi.font:                           terminalfont 10
rofi.bw:                             5
rofi.location:                       0
rofi.padding:                        5
rofi.yoffset:                        100
rofi.xoffset:                        0
rofi.fixed-num-lines:                false
rofi.show-icons:                     false
rofi.terminal:                       rofi-sensible-terminal
rofi.ssh-client:                     ssh
rofi.ssh-command:                    {terminal} -e '{ssh-client} {host}'
rofi.run-command:                    {cmd}
rofi.run-list-command:
rofi.run-shell-command:              {terminal} -e {cmd}
rofi.window-command:                 xkill -id {window}
rofi.window-match-fields:            all
rofi.icon-theme:
rofi.drun-match-fields:              name,generic,exec,categories
rofi.drun-show-actions:              true
rofi.drun-display-format:            {name} [<span weight='light' size='small'><i>({generic})</i></span>]
rofi.disable-history:                false
rofi.sort:                           true
rofi.sorting-method:                 normal
rofi.case-sensitive:                 false
rofi.cycle:                          true
rofi.sidebar-mode:                   true
rofi.eh:                             1
rofi.auto-select:                    false
rofi.parse-hosts:                    false
rofi.parse-known-hosts:              true
rofi.matching:                       glob
rofi.tokenize:                       true
rofi.m:                              -5
rofi.filter:
rofi.fullscreen:                     false
rofi.threads:                        0
rofi.scroll-method:                  1
rofi.fake-background:                screenshot
rofi.click-to-exit:                  true
rofi.show-match:                     true
rofi.max-history-size:               25
rofi.combi-hide-mode-prefix:         false
rofi.matching-negate-char:           -
rofi.pid:                            /run/user/1000/rofi.pid

